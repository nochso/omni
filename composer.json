{
    "name": "nochso/omni",
    "description": "Helps with every day nuisances like path or EOL handling.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Voigt",
            "email": "mv@noch.so"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/polyfill-mbstring": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "nochso\\Omni\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nochso\\Omni\\Test\\": "test/"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix"
    },
    "suggest": {
        "fabpot/php-cs-fixer": "Automatically fix PHP code style issues. See namespace \\nochso\\Omni\\PhpCsFixer"
    }
}
